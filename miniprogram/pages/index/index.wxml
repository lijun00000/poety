<!--index.wxml-->
<view class="content">
  <view class="top-search">
    <input model : value="{{value}}" bindblur="blur" data-=""/>
    <button bindtap="search" type="primary" size="mini">搜索</button>
  </view>
  <view>
    <navigator url="../index/poetry-solitaire/poetry-solitaire" open-type="navigate" class="button-list"><view class="button-list-item">诗词接龙</view></navigator>
    <navigator url="../index/keywordanswer/keywordanswer" open-type="navigate" class="button-list"><view class="button-list-item">关键词答诗</view></navigator>
    <navigator url="../index/matching/matching" open-type="navigate" class="button-list"><view class="button-list-item">诗词匹配</view></navigator>
    <navigator url="../index/elocnte/elocnte" open-type="navigate" class="button-list"><view class="button-list-item">诗词朗诵</view></navigator>
    <navigator url="../index/think/think" open-type="navigate" class="button-list"><view class="button-list-item">诗词联想</view></navigator>
  </view>
  <view class="style-row content-1">
    <view>
      <icon type="info" size="34"></icon>
    </view>
    <view class="ranking">
      <text>地区排名</text>
    </view>
    <view>
      <switch checked="true"></switch>
    </view>
  </view>
  <view wx:for="{{[1,2,3,4,5,6]}}" class="style-row content-1">
    <view>
      <icon type="info" size="34"></icon>
    </view>
    <view class="ranking">
      <text>张三</text>
    </view>
    <view>
      <text>1220</text>
    </view>
  </view>
  <view class="menu">
    <navigator url="" open-type="navigate" class="menu-item"><view class="menu-item-text">地图</view></navigator>
    <navigator url="" open-type="navigate" class="menu-item"><view class="menu-item-text">论坛</view></navigator>
    <navigator url="" open-type="navigate" class="menu-item"><view class="menu-item-text">互动</view></navigator>
    <navigator url="" open-type="navigate" class="menu-item"><view class="menu-item-text">个人中心</view></navigator>
  </view>
</view>
  